{
	"i18n": true,
	"type": "tabs",
	"items":{
        "mainTab":{
            "type": "panel",
            "label": "mainSettings",
			"items": {
				"ServersettingsHeader": {
					"newLine": true,
					"type": "header",
					"text": "ServersettingsHeader",
					"size": 3
				},
				"Serverinformation":{
					"newLine":true,
					"type": "staticText",
					"label": "Serverinformation"
				},
				"ipUrl":{
					"newLine": true,
					"type": "text",
					"label": "ipUrl",
					"tooltip": "ipUrlTooltip",
					"default": "",
					"sm": 4
				},
				"port":{
					"type": "number",
					"label": "port",
					"tooltip": "portTooltip",
					"default": 8883,
					"sm": 1
				},
				"ssl":{
					"type": "checkbox",
					"label": "SSL",
					"tooltip": "sslTooltip",
					"default": true
				},
				"AuthenticationHeader": {
					"newLine": true,
					"type": "header",
					"text": "AuthenticationHeader",
					"size": 3
				},
				"AuthenticationInformation":{
					"newLine":true,
					"type": "staticText",
					"label": "AuthenticationInformation"
				},
				"username":{
					"newLine":true,
					"type": "text",
					"label": "username",
					"tooltip": "usernameTooltip",
					"default": "",
					"sm": 4
				},
				"password":{
					"type": "password",
					"label": "password",
					"tooltip": "passwordTooltip",
					"default": "",
					"sm": 3
				},
				"OriginHeader": {
					"newLine": true,
					"type": "header",
					"text": "OriginHeader",
					"size": 3
				},
				"OriginInformation":{
					"newLine":true,
					"type": "staticText",
					"label": "OriginInformation"
				},
				"origin":{
					"newLine": true,
					"type": "select",
					"label": "origin",
					"tooltip": "originTooltip",
					"options": [
						{"label":"TTN","value":"ttn"},
						{"label":"Chirpstack","value":"chirpstack"}
					],
					"default": "ttn",
					"sm":1
				}
			}
		},
		"downlinkConfigTab":{
            "type": "panel",
            "label": "downlinkConfig",
			"items": {
				"downlinkConfig":{
					"type":"accordion",
					"titleAttr": "deviceType",
					"clone": true,
					"sm":12,
					"items":[
						{
							"newLine": true,
							"type": "header",
							"text": "downlinkConfigMainHeader",
							"size": 3
						},
						{
							"type": "autocomplete",
							"attr": "deviceType",
							"label": "deviceType",
							"tooltip": "deviceTypeTooltip",
							"options": [
								{"label":"all","value":"all"}
							],
							"validator": "if(data.deviceType === '' || data.deviceType === null){return false;}else{return true;}",
							"validatorNoSaveOnError": true,
							"default": "all",
							"freeSolo": true,
							"sm":2
						},
						{
							"newLine": true,
							"type": "select",
							"attr": "sendWithUplink",
							"label": "sendWithUplink",
							"tooltip": "sendWithUplinkTooltip",
							"options": [
								{"label":"disabled","value":"disabled"},
								{"label":"enabled","value":"enabled"},
								{"label":"enabled & collect","value":"enabled & collect"}
							],
							"default": "disabled",
							"sm":2
						},
						{
							"type": "number",
							"attr": "port",
							"label": "portDownlink",
							"tooltip": "portDownlinkTooltip",
							"hidden": "data.sendWithUplink === 'disabled'",
							"default": 2,
							"sm":2
						},
						{
							"type": "text",
							"attr": "priority",
							"label": "priority",
							"tooltip": "priorityTooltip",
							"hidden": "data.sendWithUplink === 'disabled'",
							"default": "NORMAL",
							"sm":2
						},
						{
							"type": "checkbox",
							"attr": "confirmed",
							"label": "confirmed",
							"tooltip": "confirmedTooltip",
							"hidden": "data.sendWithUplink === 'disabled'",
							"default": false,
							"sm":2
						},
						{
							"newLine": true,
							"type": "header",
							"text": "downlinkConfigSubHeader",
							"size": 3
						},
						{
							"type":"accordion",
							"attr":"downlinkParameter",
							"titleAttr": "name",
							"clone": true,
							"sm":12,
							"items":[
								{
									"type": "text",
									"attr": "name",
									"label": "name",
									"tooltip": "nameTooltip",
									"default": "",
									"validator": "if(data.name === '' || data.name === null){return false;}else{return true;}",
									"sm":2
								},
								{
									"type": "number",
									"attr": "port",
									"label": "portDownlink",
									"tooltip": "portDownlinkTooltip",
									"default": 2,
									"sm":2
								},
								{
									"type": "text",
									"attr": "priority",
									"label": "priority",
									"tooltip": "priorityTooltip",
									"default": "NORMAL",
									"sm":2
								},
								{
									"type": "select",
									"attr": "type",
									"label": "type",
									"tooltip": "typeTooltip",
									"options": [
										{"label":"button","value":"button"},
										{"label":"boolean","value":"boolean"},
										{"label":"number","value":"number"},
										{"label":"ASCII","value":"ascii"},
										{"label":"String","value":"string"}
									],
									"default": "boolean",
									"sm":2
								},
								{
									"type": "checkbox",
									"attr": "swap",
									"label": "swap",
									"tooltip": "swapTooltip",
									"hidden": "data.type !== 'number'",
									"default": false,
									"sm":2
								},
								{
									"type": "staticText",
									"label": "",
									"hidden": "data.type === 'number'",
									"sm":2
								},
								{
									"type": "checkbox",
									"attr": "confirmed",
									"label": "confirmed",
									"tooltip": "confirmedTooltip",
									"default": false,
									"sm":2
								},
								{
									"newLine": true,
									"type": "text",
									"attr": "front",
									"label": "front",
									"tooltip": "frontTooltip",
									"default": "03",
									"validator": "if(data.type !== 'string'){const myRegEx = /^([0-9a-f]{2})*$/i; return myRegEx.test(data.front)}else{return true}",
									"hidden": "data.type === 'boolean' || data.type === 'button'",
									"sm":2
								},
								{
									"type": "text",
									"attr": "end",
									"label": "end",
									"tooltip": "endTooltip",
									"default": "11",
									"validator": "if(data.type !== 'string'){const myRegEx = /^([0-9a-f]{2})*$/i; return myRegEx.test(data.front)}else{return true}",
									"hidden": "data.type === 'boolean' || data.type === 'button'",
									"sm":2
								},
								{
									"type": "number",
									"attr": "lengthInByte",
									"label": "lengthInByte",
									"tooltip": "lengthInByteTooltip",
									"default": 2,
									"min":1,
									"max": 20,
									"hidden": "data.type === 'boolean' || data.type === 'button' || data.type === 'string'",
									"sm":2
								},
								{
									"newLine": true,
									"type": "text",
									"attr": "on",
									"label": "on",
									"tooltip": "onTooltip",
									"default": "01",
									"validator": "const myRegEx = /^([0-9a-f]{2})*$/i; return myRegEx.test(data.on);",
									"hidden": "data.type !== 'boolean'",
									"sm":2
								},
								{
									"type": "text",
									"attr": "off",
									"label": "off",
									"tooltip": "offTooltip",
									"default": "11",
									"validator": "const myRegEx = /^([0-9a-f]{2})*$/i; return myRegEx.test(data.off);",
									"hidden": "data.type !== 'boolean'",
									"sm":2
								},
								{
									"newLine": true,
									"type": "text",
									"attr": "onClick",
									"label": "onClick",
									"tooltip": "onClickTooltip",
									"default": "030111",
									"validator": "const myRegEx = /^([0-9a-f]{2})*$/i; return myRegEx.test(data.onClick);",
									"hidden": "data.type !== 'button'",
									"sm":2
								},
								{
									"type": "number",
									"attr": "multiplyfaktor",
									"label": "multiplyfaktor",
									"tooltip": "multiplyfaktorTooltip",
									"default": "1",
									"hidden": "data.type !== 'number'",
									"sm":2
								},
								{
									"type": "number",
									"attr": "decimalPlaces",
									"label": "decimalPlaces",
									"tooltip": "decimalPlacesTooltip",
									"default": "0",
									"min": 0,
									"max": 5,
									"hidden": "data.type !== 'number'",
									"sm":1
								},
								{
									"type": "text",
									"attr": "unit",
									"label": "unit",
									"tooltip": "unitTooltip",
									"default": "",
									"hidden": "data.type === 'boolean' || data.type === 'button' || data.type === 'string'",
									"sm":1
								},
								{
									"type": "select",
									"attr": "crc",
									"label": "crc",
									"tooltip": "crcTooltip",
									"options": [
										{"label":"no crc","value":"noCrc"},
										{"label":"KERMIT","value":"KERMIT"},
										{"label":"KERMIT (Little Endian)","value":"KERMIT.LittleEndian"}
									],
									"default": "off",
									"sm":2
								}
							]
						}
					]
				}
			}
		}
	}
}